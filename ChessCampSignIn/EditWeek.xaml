<Window x:Class="ChessCampSignIn.EditWeek"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ChessCampSignIn"
        mc:Ignorable="d"
        Title="CreateNewWeek" Height="300"
		Width="400
		">
	<Grid>
		<Grid.ColumnDefinitions>
			<ColumnDefinition></ColumnDefinition>
			<ColumnDefinition Width="auto"></ColumnDefinition>
		</Grid.ColumnDefinitions>
		<DataGrid x:Name="DataElement"></DataGrid>
		<Grid Grid.Column="1">
			<Grid.RowDefinitions>
				<RowDefinition></RowDefinition>
				<RowDefinition></RowDefinition>
				<RowDefinition></RowDefinition>
			</Grid.RowDefinitions>
			<Button>Submit</Button>

			<Button Grid.Row="1"
					Margin="0">Add from file</Button>
			<Button Grid.Row="2">Delete All</Button>
		</Grid>
	</Grid>
</Window>
